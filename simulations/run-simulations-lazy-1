#!/bin/bash
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-test.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 1 --window-jump 100 --run-id 1_1 --batch-size 1000 --number-window-extents 100" > output/1_1_no-migration.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-test.yaml --experiment-id 4 --migration-mechanism all-at-once --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 1 --window-jump 100 --run-id 1_1 --batch-size 1000 --number-window-extents 100" > output/1_1_all-at-once.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-test.yaml --experiment-id 4 --migration-mechanism rhino --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 1 --window-jump 100 --run-id 1_1 --batch-size 1000 --number-window-extents 100" > output/1_1_rhino.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-test.yaml --experiment-id 4 --migration-mechanism lazy --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 1 --window-jump 100 --run-id 1_1 --batch-size 1000 --number-window-extents 100" > output/1_1_lazy.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-test.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 10 --window-jump 100 --run-id 1_10 --batch-size 1000 --number-window-extents 100" > output/1_10_no-migration.nohup 2>&1 &disown
