nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-test.yaml --experiment-id 4 --migration-mechanism rhino --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 10 --window-jump 100 --run-id 1_10 --batch-size 1000 --number-window-extents 100" > output/1_10_rhino.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-test.yaml --experiment-id 4 --migration-mechanism lazy --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 10 --window-jump 100 --run-id 1_10 --batch-size 1000 --number-window-extents 100" > output/1_10_lazy.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-test.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 40 --window-jump 100 --run-id 1_40 --batch-size 1000 --number-window-extents 100" > output/1_40_no-migration.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-test.yaml --experiment-id 4 --migration-mechanism all-at-once --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 40 --window-jump 100 --run-id 1_40 --batch-size 1000 --number-window-extents 100" > output/1_40_all-at-once.nohup 2>&1 &disown
