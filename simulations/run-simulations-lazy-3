#!/bin/bash
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 3 --window-jump 500 --run-id 3_3 --batch-size 1000 --number-window-extents 100" > output/3_3_no-migration.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism all-at-once --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 3 --window-jump 500 --run-id 3_3 --batch-size 1000 --number-window-extents 100" > output/3_3_all-at-once.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism rhino --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 3 --window-jump 500 --run-id 3_3 --batch-size 1000 --number-window-extents 100" > output/3_3_rhino.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism lazy --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 3 --window-jump 500 --run-id 3_3 --batch-size 1000 --number-window-extents 100" > output/3_3_lazy.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism meces --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 3 --window-jump 500 --run-id 3_3 --batch-size 1000 --number-window-extents 100" > output/3_3_meces.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism megaphone --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 3 --window-jump 500 --run-id 3_3 --batch-size 1000 --number-window-extents 100" > output/3_3_megaphone.nohup 2>&1 &disown


nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 500 --run-id 3_5 --batch-size 1000 --number-window-extents 100" > output/3_5_no-migration.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism all-at-once --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 500 --run-id 3_5 --batch-size 1000 --number-window-extents 100" > output/3_5_all-at-once.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism rhino --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 500 --run-id 3_5 --batch-size 1000 --number-window-extents 100" > output/3_5_rhino.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism lazy --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 500 --run-id 3_5 --batch-size 1000 --number-window-extents 100" > output/3_5_lazy.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism meces --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 500 --run-id 3_5 --batch-size 1000 --number-window-extents 100" > output/3_5_meces.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism megaphone --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 500 --run-id 3_5 --batch-size 1000 --number-window-extents 100" > output/3_5_megaphone.nohup 2>&1 &disown


nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 10 --window-jump 500 --run-id 3_10 --batch-size 1000 --number-window-extents 100" > output/3_10_no-migration.nohup 2>&1 &disown
