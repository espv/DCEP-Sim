#!/bin/bash
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test-2.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 100 --run-id 4_100 --batch-size 1000 --number-window-extents 100" > output/4_100_no-migration.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test-2.yaml --experiment-id 4 --migration-mechanism all-at-once --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 100 --run-id 4_100 --batch-size 1000 --number-window-extents 100" > output/4_100_all-at-once.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test-2.yaml --experiment-id 4 --migration-mechanism rhino --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 100 --run-id 4_100 --batch-size 1000 --number-window-extents 100" > output/4_100_rhino.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test-2.yaml --experiment-id 4 --migration-mechanism lazy --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 100 --run-id 4_100 --batch-size 1000 --number-window-extents 100" > output/4_100_lazy.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test-2.yaml --experiment-id 4 --migration-mechanism meces --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 100 --run-id 4_100 --batch-size 1000 --number-window-extents 100" > output/4_100_meces.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test-2.yaml --experiment-id 4 --migration-mechanism megaphone --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 100 --run-id 4_100 --batch-size 1000 --number-window-extents 100" > output/4_100_megaphone.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-test-2.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 5 --window-jump 200 --run-id 4_200 --batch-size 1000 --number-window-extents 100" > output/4_200_no-migration.nohup 2>&1 &disown
