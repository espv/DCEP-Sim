#!/bin/bash
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-max-utility-test.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 5000 --run-id 5_1 --batch-size 1000 --number-window-extents 10 --disconnection-time 1" > output/5_1_no-migration.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-max-utility-test.yaml --experiment-id 4 --migration-mechanism utility --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 5000 --run-id 5_1 --batch-size 1000 --number-window-extents 10 --disconnection-time 1" > output/5_1_utility.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-max-utility-test.yaml --experiment-id 4 --migration-mechanism lazy --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 5000 --run-id 5_1 --batch-size 1000 --number-window-extents 10 --disconnection-time 1 --disconnection-time 1" > output/5_1_lazy.nohup 2>&1 &disown

nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-max-utility-test.yaml --experiment-id 4 --migration-mechanism rhino --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 5000 --run-id 5_1 --batch-size 1000 --number-window-extents 10 --disconnection-time 1" > output/5_1_rhino.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-max-utility-test.yaml --experiment-id 4 --migration-mechanism meces --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 5000 --run-id 5_1 --batch-size 1000 --number-window-extents 10 --disconnection-time 1" > output/5_1_meces.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-max-utility-test.yaml --experiment-id 4 --migration-mechanism megaphone --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 5000 --run-id 5_1 --batch-size 1000 --number-window-extents 10 --disconnection-time 1" > output/5_1_megaphone.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-max-utility-test.yaml --experiment-id 4 --migration-mechanism all-at-once --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 5000 --run-id 5_1 --batch-size 1000 --number-window-extents 10 --disconnection-time 1" > output/5_1_all-at-once.nohup 2>&1 &disown



nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-max-utility-test.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 5000 --run-id 5_500 --batch-size 1000 --number-window-extents 10 --disconnection-time 500" > output/5_500_no-migration.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-max-utility-test.yaml --experiment-id 4 --migration-mechanism utility --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 5000 --run-id 5_500 --batch-size 1000 --number-window-extents 10 --disconnection-time 500" > output/5_500_utility.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-max-utility-test.yaml --experiment-id 4 --migration-mechanism lazy --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 5000 --run-id 5_500 --batch-size 1000 --number-window-extents 10 --disconnection-time 500" > output/5_500_lazy.nohup 2>&1 &disown

nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config join-and-aggregation-max-utility-test.yaml --experiment-id 4 --migration-mechanism rhino --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 5000 --run-id 5_500 --batch-size 1000 --number-window-extents 10 --disconnection-time 500" > output/5_500_rhino.nohup 2>&1 &disown
