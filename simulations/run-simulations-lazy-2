nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config aggregation-test.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 200 --run-id 2_200 --batch-size 1000 --number-window-extents 100" > output/2_200_no-migration.nohup 2>&1 &disown
