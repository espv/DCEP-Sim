#!/bin/bash
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config aggregation-test.yaml --experiment-id 4 --migration-mechanism no-migration --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 0 --run-id 2_0 --batch-size 1000 --number-window-extents 100" > output/2_0_no-migration.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config aggregation-test.yaml --experiment-id 4 --migration-mechanism all-at-once --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 0 --run-id 2_0 --batch-size 1000 --number-window-extents 100" > output/2_0_all-at-once.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config aggregation-test.yaml --experiment-id 4 --migration-mechanism rhino --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 0 --run-id 2_0 --batch-size 1000 --number-window-extents 100" > output/2_0_rhino.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config aggregation-test.yaml --experiment-id 4 --migration-mechanism meces --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 0 --run-id 2_0 --batch-size 1000 --number-window-extents 100" > output/2_0_meces.nohup 2>&1 &disown
nohup python3 ../ns3 run --no-build "lazy-migration-minimize-latency --yaml-config aggregation-test.yaml --experiment-id 4 --migration-mechanism megaphone --tuples-between-join 1 --joins-per-tuple 1 --join-range 1 --percentage-A-tuples-join 100 --window-jump 0 --run-id 2_0 --batch-size 1000 --number-window-extents 100" > output/2_0_megaphone.nohup 2>&1 &disown
